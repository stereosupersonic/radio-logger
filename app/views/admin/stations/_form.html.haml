= form_for [:admin, @station], :html => { :class => "form-horizontal" } do |f|
  .form-group 
    = f.label :name, :class => "control-label"
    = f.text_field :name, :class => "text-field form-control", :required => true
  .form-group
    = f.label :url
    = f.text_field :url, :class => "text-field form-control", :required => true
  = submit_button "Save"
